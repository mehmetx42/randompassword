import random 
karakterler = "+-/*!&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
uzunluk= int(input("Şifre (sayi)uzunluğu: "))

passw=""
for i in range(uzunluk):
    karakter=random.choice(karakterler)
    passw += karakter
    print(passw)
    karakterler.
    print(karakterler)

print(passw)
